    "cross-fetch": "3.1.4",
    "extract-files": "11.0.0",
    "form-data": "4.0.0",
    "graphql-ws": "^5.0.0",
    "is-promise": "4.0.0",
    "isomorphic-ws": "4.0.1",
    "lodash": "4.17.21",
    "meros": "1.1.4",
    "subscriptions-transport-ws": "^0.10.0",
